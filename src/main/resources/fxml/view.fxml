<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cvt.pdfuploader.controller.EventController">
   <children>
      <HBox prefHeight="30.0" prefWidth="400.0">
         <children>
            <TextField fx:id="textField" prefHeight="30" prefWidth="500" />
            <Button fx:id="button" mnemonicParsing="false" onAction="#browseOperation" prefHeight="20" prefWidth="100" text="Browse" />
         </children>
      </HBox>
      <Label fx:id="label" text="Count: 0" />
   </children>
</VBox>
